<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <OutputType>WinExe</OutputType>
        <TargetFramework>net8.0-windows</TargetFramework>
        <Nullable>enable</Nullable>
        <ImplicitUsings>enable</ImplicitUsings>
        <UseWPF>true</UseWPF>
    </PropertyGroup>

    <ItemGroup>
      <Folder Include="Imagens\" />
    </ItemGroup>

    <ItemGroup>
      <None Remove="Imagens\Combustível.jpg" />
      <Resource Include="Imagens\Combustível.jpg">
        <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      </Resource>
      <None Remove="Imagens\Comb.png" />
      <Resource Include="Imagens\Comb.png">
        <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      </Resource>
      <None Remove="Imagens\fuel-gas.png" />
      <Resource Include="Imagens\fuel-gas.png">
        <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      </Resource>
    </ItemGroup>

    <ItemGroup>
      <PackageReference Include="System.IO.Ports" Version="10.0.0-preview.7.25380.108" />
    </ItemGroup>

</Project>
